{{#each pageContent as |item|}}
  {{#if item.masthead}} 
    {{generic-bar content=item.masthead}}
  {{/if}}
  {{#if item.incomeForm}}
    {{generic-form formContent=offerData.form.incomeForm}}
  {{/if}}
  {{#if item.actionPanel}}
    {{action-panel target=offerData.form.incomeForm}}
  {{/if}}
{{/each}}
